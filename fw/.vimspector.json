{
    "adapters": {
        "probe-rs": {
            "command": ["probe-rs"],
            "name": "probe-rs",
            "port": "56000",
            "type": "server",
            "host": "127.0.0.1"
        }
    },
    "configurations": {
        "probe-rs": {
            "adapter": "probe-rs",
            "configuration": {
                "request": "launch",
                "chip": "STM32F411RE",
                "coreConfigs": [
                    {
                        "cwd": "${workspaceRoot}",
                        "programBinary": "${workspaceRoot}/target/thumbv7em-none-eabihf/debug/seq_08"
                    }
                ]
            }
        }
    }
}
